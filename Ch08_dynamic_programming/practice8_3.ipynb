   "source": [
    "n = int(input())\n",
    "k = list(map(int, input().split()))\n",
    "d = [0]*101\n",
    "\n",
    "for i in range(n-2):\n",
    "    for j in range(i+2, n):\n",
    "        d[i] = max(d[i], k[i] + k[j])\n",
    "d[n-2] = k[n-2]\n",
    "\n",
    "print(max(d))"
